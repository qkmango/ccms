<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.qkmango.ccms.mvc.dao.TradeDao">

    <insert id="insert">
        insert into t_trade (id,
                             account,
                             level1,
                             level2,
                             level3,
                             state,
                             out_id,
                             in_id,
                             amount,
                             create_time,
                             description,
                             version)
        values (#{id},
                #{account},
                #{level1},
                #{level2},
                #{level3},
                #{state},
                #{outId},
                #{inId},
                #{amount},
                #{createTime},
                #{description},
                #{version})
    </insert>
</mapper>